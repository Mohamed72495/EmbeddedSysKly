#include <math.h>

unsigned int distanceBetweenTwoPoints_Harvesine(float lat_1, float long_1, float lat_2, float long_2)
{
 int D_EarthDiameter = 2 * 6371000;
 float lat_differ = lat_2 - lat_1;
 float long_differ = long_2 - long_1;
 float under_sqrt = pow(sin(lat_differ / 2), 2) + cos(lat_1)*cos(lat_2)*pow(sin(long_differ / 2), 2);

 float flt_sqrt = sqrtf(under_sqrt);
 return (int)(D_EarthDiameter*asinf(flt_sqrt));
}
 int main(int argc, char const *argv[]) {
  /* code */

distanceBetweenTwoPoints_Harvesine(30.002350,31.178291,30.003019,31.177931)




  return 0;
}
